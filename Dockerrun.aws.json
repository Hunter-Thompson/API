{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "api",
            "image": "hunterthompson/api",
            "hostname": "api",
            "essential": false,
            "memory": "128"
        },
        {
            "name": "postgres",
            "image": "hunterthompson/postgres",
            "hostname": "postgres",
            "essential": false,
            "memory": "128"
        },
        {
            "name": "nginx",
            "image": "hunterthompson/nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": "128",
            "portMappings": [
            {
                "hostPort": 3050,
                "containerPort": 80
            }
            ],
            "links": ["api", "postgres"]
        }

    ]
}